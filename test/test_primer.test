#!/bin/sh

MD5TOOL=./file_md5


if ! ./test_primer /tmp/libmxf_test.mxf
then
  rm -f /tmp/libmxf_test.mxf
  exit 1
fi


$MD5TOOL < /tmp/libmxf_test.mxf > /tmp/libmxf_test.mxf.md5
if diff /tmp/libmxf_test.mxf.md5 ${srcdir}/test_primer.md5
then
	RESULT=0
else
	RESULT=1
fi


rm -f /tmp/libmxf_test.mxf /tmp/libmxf_test.mxf.md5


exit $RESULT

